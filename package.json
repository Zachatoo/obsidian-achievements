{
	"name": "obsidian-achievements",
	"version": "0.0.9",
	"description": "Add achievements to Obsidian.",
	"main": "src/main.js",
	"scripts": {
		"dev": "npm run wasm && node esbuild.config.mjs",
		"test": "vitest",
		"test:ci": "vitest run",
		"typecheck": "tsc -noEmit -skipLibCheck",
		"lint": "npx eslint . --ext .ts,.svelte",
		"wasm": "wasm-pack build --target web",
		"build": "npm run wasm && tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"prepare": "husky install"
	},
	"keywords": [
		"obsidian-plugin"
	],
	"author": "Zachatoo",
	"license": "MIT",
	"devDependencies": {
		"@popperjs/core": "^2.11.6",
		"@tsconfig/svelte": "^3.0.0",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "^5.40.1",
		"@typescript-eslint/parser": "^5.40.1",
		"builtin-modules": "3.3.0",
		"esbuild": "0.14.47",
		"esbuild-svelte": "^0.7.1",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^7.0.0",
		"obsidian": "latest",
		"obsidian-svelte": "^0.0.28",
		"svelte": "^3.52.0",
		"svelte-portal": "^2.2.0",
		"svelte-preprocess": "^4.10.7",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"vitest": "^0.24.3"
	},
	"dependencies": {
		"monkey-around": "^2.3.0"
	}
}
